<Window x:Class="SistemaCriptografado.TelaCadastroServico"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SistemaCriptografado"
        mc:Ignorable="d"
        Title="TelaCadastroServico" Height="300" Width="300">
    <Grid Margin="0,0,2,142">
        <Label x:Name="label" Content="Usuário" HorizontalAlignment="Left" Margin="20,24,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Servico" HorizontalAlignment="Left" Margin="22,55,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.286,-0.286"/>
        <Label x:Name="label2" Content="Senha" HorizontalAlignment="Left" Margin="22,86,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="103,24,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtServico" HorizontalAlignment="Left" Height="23" Margin="103,55,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtSenha" HorizontalAlignment="Left" Height="23" Margin="103,88,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
